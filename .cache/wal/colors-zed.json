{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c5",
                "text": "#c3c4c5",
                "text.muted": "#c3c4c570",
                "text.ignored": "#c3c4c540",
                "text.placeholder": "#c3c4c550",
                "ignored": "#c3c4c530",
                "element.hover": "#c3c4c530",
                "ghost_element.hover": "#3B698A30",
                "ghost_element.selected": "#3B698A30",
                "ghost_element.active": "#3B698A60",
                "border": "#000000",
                "editor.highlighted_line.background": "#3B698A10",
                "editor.active_line.background": "#3B698A10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#3B698A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#3B698A",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c5",
                "terminal.dim_foreground": "#c3c4c5",
                "terminal.bright_foreground": "#c3c4c5",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#3B698A",
                "terminal.ansi.green": "#3F77A1",
                "terminal.ansi.yellow": "#477796",
                "terminal.ansi.blue": "#487EA1",
                "terminal.ansi.magenta": "#6194AD",
                "terminal.ansi.cyan": "#5F9DC1",
                "terminal.ansi.white": "#c3c4c5",
                "terminal.ansi.bright_black": "#5c6470",
                "terminal.ansi.bright_red": "#3B698A",
                "terminal.ansi.bright_green": "#3F77A1",
                "terminal.ansi.bright_yellow": "#477796",
                "terminal.ansi.bright_blue": "#487EA1",
                "terminal.ansi.bright_magenta": "#6194AD",
                "terminal.ansi.bright_cyan": "#5F9DC1",
                "terminal.ansi.bright_white": "#c3c4c5",
                "modified": "#3B698A",
                "syntax": {
                    "attribute": {
                        "color": "#3B698A"
                    },
                    "boolean": {
                      "color": "#3F77A1"
                    },
                    "comment": {
                      "color": "#5c6470"
                    },
                    "comment.doc": {
                      "color": "#5c6470"
                    },
                    "constant": {
                      "color": "#3F77A1"
                    },
                    "function": {
                      "color": "#5F9DC1"
                    },
                    "keyword": {
                      "color": "#487EA1"
                    },
                    "number": {
                      "color": "#6194AD"
                    },
                    "operator": {
                      "color": "#487EA1"
                    },
                    "string": {
                      "color": "#3B698A"
                    },
                    "variable": {
                      "color": "#3F77A1"
                    }
                },
                "players": [
                  {
                    "cursor": "#3B698A",
                    "background": "#000000",
                    "selection": "#3B698A30"
                  }
                ]
            }
        }
    ]
}
